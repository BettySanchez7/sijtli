{
  "name": "sijtli",
  "version": "0.1.0",
  "private": true,
  "description": "Cuéntamelo Sijtli - Hackathon Innovaccion Virtual",
  "homepage": "www.sijtli.me",
  "repository": "github:daviddesmet/sijtli",
  "author": "David De Smet <viajero08438@innovaccion.mx>",
  "contributors": [
    "María Beatriz Sánchez Díaz <viajero02831@innovaccion.mx>",
    "José Francisco Cruz Camacho <viajero03301@innovaccion.mx>",
    "Rubén Rojas Román <viajero26466@innovaccion.mx>",
    "María del Rosario Vargas Tecuatl <viajero25605@innovaccion.mx>"
  ],
  "keywords": [
    "innovaccion virtual",
    "azure",
    "machine learning",
    "ai"
  ],
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:staging": "vue-cli-service build --mode staging",
    "build:ci": "yarn build --report",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,ts,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --headless",
    "test": "run-s test:unit test:e2e",
    "test:ci": "run-s test:unit:ci test:e2e"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.10",
    "@amcharts/amcharts4-geodata": "^4.1.18",
    "core-js": "^3.7.0",
    "lodash-es": "^4.17.15",
    "microsoft-cognitiveservices-speech-sdk": "^1.14.0",
    "tailwindcss": "^1.9.6",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/lodash-es": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-e2e-cypress": "~4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.8",
    "@vue/cli-plugin-router": "~4.5.8",
    "@vue/cli-plugin-typescript": "~4.5.8",
    "@vue/cli-plugin-unit-jest": "~4.5.8",
    "@vue/cli-plugin-vuex": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "typescript": "~4.0.5",
    "vue-jest": "^5.0.0-0"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  }
}
